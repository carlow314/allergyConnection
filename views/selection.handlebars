<div class="container-fluid">
  <header class="main-header">
    <img class="furbaby-logo" src="assets/images/logo.png" alt="Our logo">
</div>
<div class="container">
  {{users}}
  <div id="welcome" class="center-block well">
    <h3 class="text-center">Welcome {{Firstname}}! Are you a dog or cat lover?</h3>
  </div>
</div>
<div class="container">
  <div class="row">
    <div class="col-lg-6">
      <form action="/dogdashboard">
        <input type="image" id="dogbutton" value="send" class="btn" src="assets/images/dogs-button.png">
      </form>
      <div class="col-lg-12">
        <div class="well text-center"> <span id="dogcounter">{{dogCount}}</span>
        </div>
      </div>
    </div>
    <div class="col-lg-6">
      <form action="/catdashboard">
        <input type="image" id="catbutton" value="send" class="btn" src="assets/images/cats-button.png">
      </form>
      <div class="col-lg-12">
        <div class="well text-center"> <span id="catcounter">{{catCount}}</span>
        </div>
      </div>
    </div>
  </div>
</div>